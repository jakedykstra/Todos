{
  "name": "todos",
  "version": "1.0.0",
  "description": "Developing a application for developing a todo list",
  "main": "server/index.js",
  "engines": {
    "node": "8.1.1",
    "npm": "5.0.3"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server/index.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jakedykstra/Todos.git"
  },
  "author": "Jacob Dykstra",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jakedykstra/Todos/issues"
  },
  "homepage": "https://github.com/jakedykstra/Todos#readme",
  "dependencies": {
    "bcrypt-nodejs": "*",
    "express-session": "^1.11.3",
    "body-parser": "^1.17.2",
    "concurrently": "^3.5.0",
    "cors": "^2.8.4",
    "create-react-app": "^1.5.2",
    "dotenv": "^6.0.0",
    "express": "^4.17.3",
    "localtunnel": "^1.8.3",
    "lodash": "^4.17.4",
    "mongoose": "^4.11.1",
    "morgan": "*",
    "nodemon": "^1.11.0",
    "path-parser": "^2.0.2",
    "sendgrid": "^5.1.2",
    "stripe": "^4.23.1"
  }
}
